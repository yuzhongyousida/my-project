<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
    ">
     
    <!-- 当前应用信息配置 -->
    <dubbo:application name="A-server" />
    
    <!-- 连接注册中心配置 -->
    <dubbo:registry id="reg1" address="127.0.0.1:4545" />
    <dubbo:registry id="reg2" address="127.0.0.1:4546" />
    <dubbo:registry id="reg3" address="127.0.0.1:4547" default="false" />
     
    <!-- 暴露服务协议配置 -->
    <dubbo:protocol name="dubbo" port="20880" />
    
    <!-- 暴露服务配置 -->
    <dubbo:service interface="com.myself.service.AdemoService" ref="ademoService" registry="reg2" />
     
    <bean id="ademoService" class="com.myself.dubbo.AdemoServiceImpl" />
 
</beans>